    }
    .embed-games-mod-mask{
        visibility: hidden;
        display: block;
        width: 100%;
        height: 100%;
        position: absolute;
        left: 0;
        top: 0;
    }
    .embed-games-mod-maskbg{
        visibility: hidden;
        display: block;
        position: absolute;
        width: 100%;
        height: 100%;
        background-color: #000;
